        </tree>
      </field>
    </record>

    <record id="producto_action" model="ir.actions.act_window">
      <field name="name">producto</field>
      <field name="res_model">producto</field>
      <field name="view_mode">tree,form</field>
    </record>

    <menuitem
      id="producto_menu"
      name="producto"
      action="producto_action"
      sequence="11"/>


